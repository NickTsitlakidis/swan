{
    "extends": [
        "../../.eslintrc.json"
    ],
    "ignorePatterns": [
        "!**/*"
    ],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "rules": {
                "@typescript-eslint/no-empty-function": "warn"
            }
        },
        {
            "files": ["*.ts", "*.tsx"],
            "rules": {
                "@typescript-eslint/no-empty-function": "warn"
            }
        },
        {
            "files": ["*.js", "*.jsx"],
            "rules": {
                "@typescript-eslint/no-empty-function": "warn"
            }
        },
        {
            "files": ["*.spec.ts", "**/test-utils/*"],
            "rules": {
                "@typescript-eslint/no-empty-function": "off",
                "@typescript-eslint/no-explicit-any": "off"
            }
        }
    ]
}
