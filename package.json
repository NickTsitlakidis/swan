    "license": "MIT",
    "scripts": {
        "ng": "nx",
        "postinstall": "patch-package && node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
        "legacy-install": "npm i --legacy-peer-deps",
        "start": "npm run create-solidity-types && nx serve",
        "start-api": "npm run create-solidity-types && nx serve api",
        "start-all": "concurrently --kill-others \"npm run start-api\" \"nx serve\"",
        "build": "nx build",
        "build-api": "npm run create-solidity-types && nx build api",
        "lint-api": "nx lint api",
        "lint-nft-marketplace": "nx lint nft-marketplace",
        "test": "nx test",
        "test-api": "npm run create-solidity-types && nx test api --skip-nx-cache",
        "test-solidity": "cd ./apps/solidity-contracts && npx hardhat test",
        "create-solidity-types": "cd ./apps/solidity-contracts && npx hardhat typechain"
    },
